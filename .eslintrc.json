{
  "env": {
    "browser": true,
    "node": true,
    "es2020": true,
    "es6": true
  },
  "extends": [
    "eslint:recommended",
    "prettier"
  ],
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module"
  },
  "plugins": [],
  "rules": {
    "no-console": "warn",
    "no-unused-vars": "warn"
  },
  "globals": {
    "bootstrap": "readonly"
  }
}
